// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Snippet {
  id          String @id @default(uuid())
  title       String
  category    String
  description String
  language    String
  snippet     String
}
